{
  "manifestVersion": 5,
  "id": "com.photoshop.mcp.bridge",
  "name": "Photoshop MCP Bridge",
  "version": "0.1.0",
  "main": "index.html",
  "host": [
    {
      "app": "PS",
      "minVersion": "23.3.0"
    }
  ],
  "entrypoints": [
    {
      "type": "panel",
      "id": "mcp-bridge-panel",
      "main": "index.html",
      "label": {
        "default": "MCP Bridge"
      },
      "minimumSize": {
        "width": 230,
        "height": 200
      },
      "maximumSize": {
        "width": 500,
        "height": 400
      },
      "preferredDockedSize": {
        "width": 230,
        "height": 200
      },
      "preferredFloatingSize": {
        "width": 300,
        "height": 250
      },
      "icons": []
    }
  ],
  "requiredPermissions": {
    "network": {
      "domains": "all"
    },
    "localFileSystem": "fullAccess",
    "clipboard": "readAndWrite"
  }
}
